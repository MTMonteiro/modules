config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/10180000.wmac'
        option htmode 'HT20'
        option country '00'
        option disabled '0'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option mode 'ap'
        option ssid 'REDE'
        option network 'lan'
        option encryption 'wpa2'
        option auth_server '192.168.8.1'
        option auth_port '1812'
        option auth_secret 'scifiwifi1373'

config wifi-iface
        option device 'radio0'
        option mode 'ap'
        option network 'lan'
        option encryption 'wpa2'
        option auth_server '192.168.8.1'
        option auth_port '1812'
        option auth_secret 'scifiwifi1373'
        option ssid 'BELUGA'


