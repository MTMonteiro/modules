config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'REDE.ZONE'
	option network 'lan'
	option encryption 'wpa2'
	option auth_server '104.196.48.80'
	option auth_port '1812'
	option auth_secret 'testing123'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option encryption 'wpa2'
	option auth_server '104.196.48.80'
	option auth_port '1812'
	option auth_secret 'testing123'
	option ssid 'DOMAIN'
	option disabled '1'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option encryption 'wpa2'
	option auth_server '104.196.48.80'
	option auth_port '1812'
	option auth_secret 'testing123'
	option ssid 'DOMAIN'
	option disabled '1'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option ssid 'OPEN ZONE'
	option disabled '0'
	option network 'OPEN_ZONE'

