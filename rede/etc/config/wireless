config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/10180000.wmac'
        option htmode 'HT20'
        option country '00'
        option disabled '0'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option mode 'ap'
        option ssid 'REDE.ZONE'
        option network 'lan'
        option encryption 'wpa2'
        option auth_server '104.196.48.80'
        option auth_port '1812'
        option auth_secret 'testing123'

config wifi-iface
        option device 'radio0'
        option mode 'ap'
        option network 'lan'
        option encryption 'wpa2'
        option auth_server '192.168.8.1'
        option auth_port '1912'
        option auth_secret 'scifiwifi1373'
        option ssid 'C41'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'TESTE'
	option network 'lan'
	option encryption 'psk'
	option key '12345678'

config wifi-iface              
        option device 'radio0'
        option mode 'ap'
        option encryption 'none'
        option ssid 'INFO'    
        option disabled '1'


